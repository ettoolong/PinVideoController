{
  "description": "__MSG_extDescription__",
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "0.0.2",
  "homepage_url": "https://github.com/ettoolong/PinVideoController",
  "icons": {
    "16": "icon/icon.svg",
    "32": "icon/icon.svg",
    "48": "icon/icon.svg",
    "64": "icon/icon.svg",
    "96": "icon/icon.svg"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "PinVideoController@ettoolong",
      "strict_min_version": "66.0"
    }
  },
  "developer": {
    "name": "Ett Chung",
    "url": "https://github.com/ettoolong/PinVideoController"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*","file:///*"],
      "js": ["js/detectClick.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "__MSG_extDescription__",
    "default_icon": "icon/icon_b.svg",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/*", "https://*/*", "file:///*",
    "storage",
    "activeTab"
  ],
  "default_locale": "en",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false,
    "browser_style": true
  }
}
